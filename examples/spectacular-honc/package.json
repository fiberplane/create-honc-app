{
  "name": "spectacular",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "deploy": "vite build && wrangler deploy --minify",
    "db:touch": "wrangler d1 execute honc-d1-database --local --command='SELECT 1'",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "wrangler d1 migrations apply honc-d1-database --local",
    "db:migrate:prod": "ENVIRONMENT=production drizzle-kit migrate",
    "db:seed": "tsx seed.ts",
    "db:seed:prod": "ENVIRONMENT=production tsx seed.ts",
    "db:setup": "npm run db:touch && npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:studio": "drizzle-kit studio",
    "shadd": "pnpm dlx shadcn@latest add"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.2.1",
    "@assistant-ui/react": "^0.8.4",
    "@assistant-ui/react-ai-sdk": "^0.8.0",
    "@assistant-ui/react-markdown": "^0.8.0",
    "@fiberplane/hono": "0.4.4-beta.2",
    "@hono/zod-validator": "^0.4.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "agents-sdk": "^0.0.27",
    "ai": "^4.1.53",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.39.3",
    "hono": "^4.7.0",
    "hono-agents": "^0.0.17",
    "lucide-react": "^0.477.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "0.1.7",
