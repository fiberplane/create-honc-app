{
  "name": "cf-etrieval-augmented-goose",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy --minify src/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx seed.ts",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:studio": "drizzle-kit studio",
    "fiberplane": "fpx",
    "vectors:create": "tsx --tsconfig tsconfig.scripts.json scripts/create-vectors.ts"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.1",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "hono": "^4.6.7",
    "llamaindex": "^0.8.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@fiberplane/hono-otel": "latest",
    "@fiberplane/studio": "latest",
    "@types/node": "^22.2.0",
    "drizzle-kit": "^0.26.2",
    "pg": "^8.13.1",
    "pgvector": "^0.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "typescript-language-server": "^4.3.3",
    "wrangler": "^3.83.0"
  },
  "description": "i would like to build a basic semantic search app against an index of cloudflare documentation - will use cosine similarity to compare vectors representing documentation chunks from cloudflare docs"
}