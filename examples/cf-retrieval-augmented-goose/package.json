{
  "name": "cf-retrieval-augmented-goose",
  "description": "cloudflare docs semantic search",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev src/index.tsx",
    "deploy": "wrangler deploy --minify src/index.tsx",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx seed.ts",
    "db:setup": "npm run db:generate && npm run db:migrate && npm run db:seed",
    "db:studio": "drizzle-kit studio",
    "fiberplane": "pnpx @fiberplane/studio@canary",
    "vectors:create": "tsx --tsconfig tsconfig.scripts.json scripts/create-vectors.ts",
    "format": "biome check . --write"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.72",
    "@neondatabase/serverless": "^0.10.1",
    "ai": "^3.4.33",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.35.3",
    "hono": "^4.6.7",
    "openai": "^4.71.0",
    "tslib": "^2.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241022.0",
    "@fiberplane/hono-otel": "latest",
    "@types/node": "^22.2.0",
    "drizzle-kit": "^0.26.2",
    "pg": "^8.13.1",
    "pgvector": "^0.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "typescript-language-server": "^4.3.3",
    "wrangler": "^3.83.0"
  }
}
